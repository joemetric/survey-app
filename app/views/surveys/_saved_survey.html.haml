%tr
  %td= link_to saved_survey.name, edit_survey_path(saved_survey)
  %td= saved_survey.created_at.strftime("%b/%d/%Y")
  %td= saved_survey.updated_at.strftime("%b/%d/%Y")
  %td= saved_survey.questions.size
  %td= link_to_remote 'Delete', :url => remove_survey_path(saved_survey), :complete => "showForm()", :html => { :class => "but_da" }