= form.label "Maximum # of respondents"
%p.corr
  = form.text_field :responses, :value => @object.responses ? @object.responses : 1, :class => "small_l"
  %p.cls

%br
  
= form.label :end_at
%p.corr
  = form.date_select :end_at, :order => [ :month, :day, :year ], :start_year => Time.now.year
  %p.cls
