%table{:cellpadding => 0, :cellspacing => 0, :border => 0, :id => 'table', :class => 'sortable'}
  %thead
    %tr
      %th
        %h3
          = @filter_by
      %th
        %h3 # of Users
  %tbody
    - if @filter_by == 'Age'
      - @filters.each do |f|
        %tr
          %td
            = f[0]
          %td
            = f[1]
    - else
      - @results.each do |u|
        %tr
          %td
            = @filter_by == 'Gender' ? @filters[u.filter_id] : @filters[u.filter_id.to_i]
          %td
            = u.count
      
    